  "arkham-core/library/Arkham/Investigator/Cards/*.hs": {
    "type": "investigator",
    "alternate": "arkham-core/tests/Arkham/Investigator/Cards/{}Spec.hs",
    "template": [
      "module Arkham.Investigator.Cards.{dot}",
