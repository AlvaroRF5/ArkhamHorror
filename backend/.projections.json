      "",
      "{dot|snakecase|camelcase} :: InvestigatorCard {dot}",
      "{dot|snakecase|camelcase} = investigator",
      "  {dot}",
      "  Cards.{dot|snakecase|camelcase}",
      "  Stats",
      "    { health = 0",
      "    , sanity = 0",
      "    , willpower = 0",
      "    , intellect = 0",
      "    , combat = 0",
      "    , agility = 0",
      "    }",
      "",
      "instance HasAbilities {dot} where",
      "  getAbilities ({dot} _) = []",
      "",
      "instance HasTokenValue {dot} where",
      "  getTokenValue iid ElderSign ({dot} attrs) | iid == toId attrs = do",
      "    pure $ TokenValue ElderSign NoModifier",
      "  getTokenValue _ token _ = pure $ TokenValue token mempty",
      "",
      "instance RunMessage {dot} where",
      "  runMessage msg ({dot} attrs) =",
      "    {dot} <$> runMessage msg attrs"
    ]
  },
