User json sql=users
    username Text
    email Text
    passwordDigest Text
    UniqueUsername username
    UniqueEmail email
    deriving Show

ArkhamHorrorGame json sql=arkham_horror_games
    cycleId Text
    scenarioId Text

ArkhamHorrorGamePlayers json sql=arkham_horror_game_players
    gameId ArkhamHorrorGameId
    userId UserId
    UniqueArkhamHorrorPlayer gameId userId
