User sql=users
    username Text
    email Text
    passwordDigest Text
    UniqueUsername username
    UniqueEmail email
    deriving Show

ArkhamHorrorCycle json sql=arkham_horror_cycles
    name Text

ArkhamHorrorScenario json sql=arkham_horror_scenarios
    name Text
    cycleId ArkhamHorrorCycleId
    position Int
    ScenarioCyclePosition cycleId position

ArkhamHorrorCard sql=arkham_horror_cards
    name Text
    cycle ArkhamHorrorCycle Maybe
    scenario ArkhamHorrorScenario Maybe
    number Int

ArkhamHorrorGame sql=arkham_horror_games
    gameCampaignId ArkhamHorrorGameCampaignId
    gameScenarioId ArkhamHorrorGameScenarioId
    deriving Show

ArkhamHorrorGameCampaign sql=arkham_horror_game_campaigns
    cycleId ArkhamHorrorCycleId

ArkhamHorrorGameScenario sql=arkham_horror_game_scenarios
    scenarioId ArkhamHorrorScenarioId

ArkhamHorrorGameInvestigator sql=arkham_horror_game_investigators
    gameId ArkhamHorrorGameId
    userId UserId
    GameUser gameId userId
